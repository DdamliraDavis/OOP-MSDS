    "#Using scipy.linalg.solve to model a simple supply-demand system.\n",
    "import numpy as np\n",
    "from scipy.linalg import solve\n",
    "\n",
    "#Coefficient matrix (A)\n",
    "A = np.array([[2, 1], [1, 3]])\n",
    "\n",
    "#Constants (b)\n",
    "b = np.array([220, 390])\n",
    "\n",
    "#Solve for x and y\n",
    "solution = solve(A, b)\n",
    "\n",
    "#Extract results\n",
    "x, y = solution\n",
    "\n",
    "print(f\"Quantity of good X: {x:.2f}\")\n",
    "print(f\"Quantity of good Y: {y:.2f}\")\n"
   ]
  },
